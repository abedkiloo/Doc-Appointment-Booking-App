<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ndroid="http://schemas.android.com/apk/res-auto"
    package="com.unifysoftech.abedx.medicare">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NOTIFICATION_POLICY"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/medical_bag"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.unifysoftech.abedx.medicare.DoctorCatergoriesFragment"
            android:label="@string/registeractivity"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.unifysoftech.abedxh.docbook.fragments.Register"
            android:label="@string/title_activity_doctors_panel"
            android:theme="@style/AppTheme.NoActionBar" >
            <!-- ATTENTION: This intent was auto-generated. Follow instructions at
      https://g.co/AppIndexing/AndroidStudio to publish your Android app deep links. -->
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <!-- ATTENTION: This data URL was auto-generated. We recommend that you use the HTTP scheme.
                  TODO: Change the host or pathPrefix as necessary. -->
                <data
                    android:host="medicare.abedx.unifysoftech.com"
                    android:pathPrefix="/register"
                    android:scheme="http" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.unifysoftech.abedxh.docbook.fragments.PatientLogin"
            android:label="@string/title_activity_view_hospital"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.unifysoftech.abedxh.docbook.fragments.Register" />
        </activity>
        <activity
            android:name="com.unifysoftech.abedx.medicare.CountyHospital"
            android:label="@string/title_activity_county_hospital"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name="com.unifysoftech.abedx.medicare.DoctorsPanel"
            android:label="@string/title_activity_doctor_catergories"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name="com.unifysoftech.abedxh.docbook.fragments.Diabetes"
            android:label="@string/title_activity_diabetes"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name="com.unifysoftech.abedxh.docbook.fragments.FoodPoisoning"
            android:label="@string/title_activity_food_poisoning"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name="com.unifysoftech.abedxh.docbook.fragments.Malaria"
            android:label="@string/title_activity_malaria"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name="com.unifysoftech.abedxh.docbook.fragments.Respiratory"
            android:label="@string/title_activity_respiratory"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name="com.unifysoftech.abedxh.docbook.fragments.Stroke_Heart_Disease"
            android:label="@string/title_activity_stroke__heart__disease"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name="com.unifysoftech.abedx.medicare.Pharmacies"
            android:label="@string/title_activity_pharmacies"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name="com.unifysoftech.abedx.medicare.BookAppointment"
            android:label="@string/title_activity_book_appointment"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
    </application>

</manifest>
